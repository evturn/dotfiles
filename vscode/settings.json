{
  "breadcrumbs.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorWidth": 1,
  "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.hover.delay": 800,
  "editor.largeFileOptimizations": false,
  "editor.letterSpacing": 0.05,
  "editor.lightbulb.enabled": false,
  "editor.lineHeight": 15.5,
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestionsDelay": 10,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.snippetSuggestions": "bottom",
  "editor.scrollBeyondLastColumn": 7,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "editor.wordWrapColumn": 100,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "explorer.decorations.colors": false,
  "files.associations": {
    "*.css": "postcss",
    "*.scss": "postcss",
    "*.sql": "postgres",
  },
  "files.trimTrailingWhitespace": true,
  "git.enableCommitSigning": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.suggest.names": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "javascript.validate.enable": false,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "markdown.extension.showExplorer": true,
  "markdown.extension.toc.githubCompatibility": true,
  "python.autoComplete.addBrackets": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "typescript.suggestionActions.enabled": false,
  "typescript.validate.enable": false,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-h>"],
      "after": ["<C-w>", "h"]
    }, {
      "before": ["<C-l>"],
      "after": ["<C-w>", "l"]
    }, {
      "before": ["<C-k>"],
      "after": ["<C-w>", "k"]
    }, {
      "before": ["<C-j>"],
      "after": ["<C-w>", "j"]
    }, {
      "before": ["<CR>"],
      "commands": [
        ":nohl",
      ]
    },
    {
      "before": ["H"],
      "after": ["0", "c", "w"]
    },
    {
      "before": ["w"],
      "commands": [
        "workbench.action.files.save",
      ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": [">"],
      "commands": [
        "editor.action.indentLines",
      ]
    },
    {
      "before": ["<"],
      "commands": [
        "editor.action.outdentLines",
      ]
    },
    {
      "before": ["<leader>", "s"],
      "commands": [
        "sortLines.sortLines",
        "workbench.action.files.save",
      ]
    },
  ],
  "vim.searchHighlightColor": "rgba(215, 255, 0, 0.3)",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.insert": ["#00212C", "#d7ff00"],
  "vim.statusBarColors.normal": ["#002b36", "#00ffaf"],
  "vim.statusBarColors.replace": ["#002b36", "#ff0087"],
  "vim.statusBarColors.visual": ["#002b36", "#5fafff"],
  "vim.statusBarColors.visualblock": ["#002b36", "#5fafff"],
  "vim.statusBarColors.visualline": ["#002b36", "#5fafff"],
  "vim.useSystemClipboard": true,
  "window.restoreWindows": "all",
  "window.title": "${activeEditorMedium}",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "editorWarning.foreground": "#005a6f",
    "editorMarkerNavigationWarning.background": "#5fd7ff",
    "statusBar.background": "#002b36",
    "statusBar.noFolderBackground": "#002b36",
    "statusBar.debuggingBackground": "#002b36",
    "statusBar.foreground": "#00ffaf"
  },
  "workbench.colorTheme": "Cosmic Barf",
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.showTabs": true,
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "seti",
  "workbench.settings.openDefaultSettings": false,
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tips.enabled": false,
  "zenMode.hideStatusBar": false,
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.quickSuggestions": false
  },
}